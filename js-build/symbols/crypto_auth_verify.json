{
	"name": "crypto_auth_verify",
	"dependencies": ["_crypto_auth_keybytes", "_crypto_auth_bytes"],
	"type": "function",
	"noEncoding": true,
	"inputs": [
		{
			"name": "tag",
			"type": "buf",
			"size": "libsodium._crypto_auth_bytes()"
		},
		{
			"name": "message",
			"type": "unsized_buf"
		},
		{
			"name": "key",
			"type": "buf",
			"size": "libsodium._crypto_auth_keybytes()"
		}
	],
	"outputs": [],
	"target": "var result = libsodium._crypto_auth_verify(tag_byte, message_byte, message_size, 0, key_byte) | 0",
	"return": "result === 0"
}
